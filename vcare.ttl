vcare:CKDSimulation
  rdf:type vcare:ClinicalPathwaySimulation ;
  vcare:hasSPARQLQuery """PREFIX vcare: <http://vcare.eu/vcare#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
SELECT DISTINCT ?instances ?targets ?activities
WHERE { 
?states rdfs:subClassOf vcare:State.
?instances rdf:type ?states.
?cp rdf:type vcare:ClinicalPathway.
?cp rdfs:label \"CKDPathway\".
?cp vcare:hasState ?instances.
?activities rdfs:subClassOf vcare:Activity.
?activity rdf:type ?activities.
?cp vcare:requiresActivity ?activity.
?targets rdf:type vcare:Target.
}""" ;
  rdfs:label "vcare CKDSimulation" ;
